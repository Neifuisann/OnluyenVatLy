# Google Gemini API Configuration
GEMINI_API_KEY=your_google_gemini_api_key_here

# Database Configuration
DATABASE_URL=postgresql://username:password@host:port/database

# Session Configuration
SESSION_SECRET=your_strong_session_secret_here_minimum_32_characters
SESSION_TIMEOUT_HOURS=72
STUDENT_SESSION_TIMEOUT_HOURS=24
ADMIN_SESSION_TIMEOUT_HOURS=72

# Application Configuration
NODE_ENV=development
CACHE_VERSION=2

# Device Security
STRICT_DEVICE_CHECK=true

# Supabase Configuration
SUPABASE_SERVICE_KEY=your_supabase_service_key_here
SUPABASE_S3_ACCESS_KEY_ID=your_s3_access_key_id_here
SUPABASE_S3_SECRET_ACCESS_KEY=your_s3_secret_access_key_here
SUPABASE_S3_ENDPOINT=https://your-project.supabase.co/storage/v1/s3
SUPABASE_S3_REGION=your-region

# Security Headers
SECURE_COOKIES=true
HTTPS_ONLY=true